#include<iostream>
#include<string.h>
using namespace std;
int main ()
{
    char a[50];
    int v=0,c=0;
    cout << "Enter a string : ";
    cin>>a;
    for (int i = 0; a[i]!='\0'; ++i)
    {
        if (a[i] == 'a' || a[i] == 'e' || a[i] == 'i' || a[i] == 'o' || a[i] == 'u' || a[i] == 'A' || a[i] == 'E' || a[i] == 'I' || a[i] == 'O' || a[i] == 'U')
            ++v;
        else;
             ++c;
    }
    cout << "Number of vowels : " << v;
    cout << "\nNumber of consonants : " << c<<endl;
    if (v>c)
    cout<<"minimum= vowels";
    else
    cout<<"minimum=constants";
    return 0;
}
